generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Member {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String   
  name      String   @db.VarChar(50)
  surname   String   @db.VarChar(50)
  mobile    Int?
  level     Int     @default(0)
  send      Int     @default(0)
  success    Int     @default(0)
  achieve    Int     @default(0)
  booking      Int     @default(0)
  client     Int     @default(0)
  createdAt DateTime @default(now())
}

model Candidate {
Id    Int      @id @default(autoincrement())
cv    Int?    
name      String?     @db.VarChar(50)
surname     String?    @db.VarChar(50)
email          String      @db.VarChar(50)
mobile         String      
position        String      @db.VarChar(50)
age            Int?         
gender          String?        @db.VarChar(10)
expect          Int?          
university1     String?        @db.VarChar(100)
major1          String?        @db.VarChar(100)
gpa1            Int?
university2     String?       @db.VarChar(100)
major2          String?       @db.VarChar(100)
gpa2            Int?
university3     String?       @db.VarChar(100)
major3          String?       @db.VarChar(100)
gpa3             Int?
skill            String?     
certificate      String?  
experience1       String?
responsibility1     String?
leave1              String?
experience2         String?
responsibility2     String?
leave2              String?
experience3         String?
responsibility3       String?
leave3            String?
experience4         String?
responsibility4       String?
leave4            String?
experience5       String?
responsibility5     String?
leave5          String?
experience6       String?
responsibility6       String?
leave6        String?
createDate   DateTime  @default(now())
updateDate    DateTime?
updateBy        String?    @db.VarChar(50)
grade     String?         @db.VarChar(10)
score      String?     @db.VarChar(50)
consent          Boolean    @default(true)
type              String?   @db.VarChar(50)
status        String?       @db.VarChar(100)
comment       String?
booking       Boolean?
bookingDate     DateTime?
bookingBy      String?        @db.VarChar(50)
startDate       DateTime?
picUrl        String?   
}